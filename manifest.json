{
	"manifest_version" : 2,
	"name" : "spaceBookMarks",
	"version" : "1.0",

	"description" : "manage bookMark with namespace",

	"browser_action": {
		"default_icon" : "icon_32.png",
		"default_popup" : "popup.html"
	},

	"permissions": [
		"bookmarks"
	],

	"js": [
		"src/jquery-2.2.0.min.js",
		"popup.js"
	],

  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
